{
  "public": "./results",
  "isProduction": false,
  "watch": true,
  "tasks": {
    "stylus": {
      "src": [
        "./develop/**/*.styl"
      ],
      "main":["./develop/styl/main.styl"],
      "dst": "css/main.css",
      "options": []
    },
    "jade": {
      "src": ["./develop/landings/**/*.jade"],
      "pages": [
        "./develop/landings/example/example.jade"
      ],
      "dst": "/html",
      "options": []
    },
    "copy": {
      "fonts": {
        "src": [
          "./develop/fonts/**/*.*"
        ],
        "dst": "./results/fonts/"
      }
    },
    "img": {
      "src": [
        "./develop/landings/example/img/*.*"
      ],
      "dst": "/images",
      "options": []
    },
    "build": [
      "stylus",
      "jade",
      "copy"
    ],
    "webServer": {
      "server": {
        "livereload": true,
        "host": "localhost",
        "port": 8085,
        "open": false,
        "fallback": "example.html"
      }
    }
  }
}
